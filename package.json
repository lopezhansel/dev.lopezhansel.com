{
  "name": "lopezhansel.com",
  "version": "1.0.0",
  "description": "Personal Website for Hansel",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts-ts build",
    "build": "mv public/web public/static && npm-run-all build-css build-js && mv build/static/ build/web/ && mv public/static public/web && sed -i 's/static/web/g' build/index.html",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "start-server": "tsc index.ts && nodemon",
    "dev": "lite-server --baseDir=public --port==4343",
    "run-prod": "pm2 start index.js --name=website"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lopezhansel/lopezhansel.com.git"
  },
  "author": "Hansel Lopez <lopezhansel@gmail.com> (http://www.lopezhansel.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lopezhansel/lopezhansel.com/issues"
  },
  "homepage": "./",
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "2.2.5",
    "express": "4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-markdown": "^3.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.13.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/react-redux": "^5.0.15",
    "@types/react-router-dom": "^4.2.4",
    "@types/classnames": "2.2.3",
    "@types/jest": "22.1.3",
    "@types/node": "9.4.6",
    "@types/react": "16.0.38",
    "@types/react-dom": "16.0.4",
    "lite-server": "^2.3.0",
    "typescript": "2.7.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "4.1.2"
  }
}
